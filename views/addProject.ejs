<% include partials/header %>
<% include partials/menu %>

<div class="container">
  <div class="row" style="padding-top: 35px;">

    <div class="alert alert-success" role="alert">
      <form action="" method="post">
        <div class="form-group row">
          <label for="inputEmail3" class="col-sm-2 col-form-label">Nama</label>
          <div class="col-sm-10">
            <input type="text" class="form-control" name="project_name" placeholder="Nama Project">
          </div>
        </div>

        <div class="form-group row">
          <label for="inputPassword3" class="col-sm-2 col-form-label">Members</label>
          <div class="col-sm-10">
            <ul class="list-unstyled">
              <% if(data.length>0){
                for(var x=0; x<data.length; x++){%>
                  <li> <input name="user" type="checkbox" value="<%= data[x].user_id %>"> <%= data[x].firstname %> <%= data[x].lastname %> </li>
                <%}
              }%>
            </ul>
          </div>
        </div>




        <div class="form-group row">
          <div class="col-sm-10">
            <button type="submit" class="btn btn-primary">Simpan</button>
          </div>
        </div>

      </form>
  </div>


</div>
</div>

</div>
<% include partials/footer %>
